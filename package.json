{
  "name": "embellish-image",
  "version": "2.0.0",
  "description": "upload image & embellish image & save image to local or server.",
  "browser": "dist/index.min.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "rollup -c rollup.config.iife.ts -w",
    "build": "rm -rf dist && yarn build-esm && yarn build-iife",
    "build-iife": "rollup -c rollup.config.iife.ts",
    "build-esm": "rollup -c rollup.config.ts && tsc",
    "server": "npx http-server ./ -p 9000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TaylorPzreal/embellish-image.git"
  },
  "keywords": [
    "upload",
    "images",
    "embellish",
    "save",
    "image"
  ],
  "author": "TaylorPzreal <taylorpzreal@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TaylorPzreal/embellish-image/issues"
  },
  "homepage": "https://github.com/TaylorPzreal/embellish-image#readme",
  "devDependencies": {
    "@babel/core": "7.8.6",
    "@babel/preset-env": "7.8.6",
    "@types/d3": "5.7.2",
    "rollup": "1.32.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.2.0",
    "rollup-plugin-typescript2": "0.26.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "d3": "5.15.0"
  }
}
